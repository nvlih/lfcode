	<script src="../../js/jquery-1.8.3.min.js"></script>
	
	<script>
	$(function(){
		// 选项卡tab
		var obj=document.getElementsByName("column");
        var rig=document.getElementsByName("ware1")
        for(var i=0;i<obj.length;i++){
            (function(i){
                obj[i].onclick=function(){
                    for(var j=0;j<rig.length;j++){
                        if(j==i){
                            rig[j].style.display="block";
                        }
                        else{
                            rig[j].style.display="none";
                        }
                    }

            }}(i))
        }
        $(".column a").click(function(){
        		 $(".column a").removeClass("active");
      			 $(this).addClass("active").siblings().removeClass("active");
		  });
        // 导航
        $(".info").toggle(function(){
			$(".pub_nav").show();
			$("#search").css("top","144px");
			$("#container").css("padding-top","195px");
			$("#left").css("top","195px")
		},function(){
			$(".pub_nav").hide();
			$("#search").css("top","46px");
			$("#container").css("padding-top","99px")
			$("#left").css("top","99px")
		})
		$(".pub_nav dl").click(function(){
			$(this).addClass("nav_bg").siblings().removeClass("nav_bg")
		});
		$("#left").scroll(function(e){
			e.stopPropagation()
		})
	})
	</script>

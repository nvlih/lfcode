	color:#d3d3d3; 
	font-size:10px;
	
}
.Shopping .fotter{ 
	width:100%; 
	height:50px; 
	background:#eeeeee; 
	position:fixed; 
	bottom:0; 
	border-top:solid 1px #dcdcdc;
}
.Shopping .fotter .left{ height:140px; float:left;}

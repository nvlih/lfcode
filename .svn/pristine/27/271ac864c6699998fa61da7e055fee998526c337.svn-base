/*购物车*/

.check-shopping {
  position: absolute;
  left: 0;
  top: 2px;
  width: 17px;
  height: 17px;
  background: url(../images/confirmed.png) 0 0 no-repeat;
  -webkit-background-size: 17px 17px;
  background-size: 17px 17px;
}
.checked-shopping {
  background: url(../images/confirm.png) 0 0 no-repeat;
  -webkit-background-size: 17px 17px;
  background-size: 17px 17px;
}
.Shopping{padding-top:46px;}
.Shopping .content, .Shopping_order .content, .my_order .content{ 
	width:100%; 
	height:auto; 
	margin-bottom:10px;
}
.Shopping .shop_cont{
	border-top:1px solid #d3d3d3; 
	margin-bottom:60px;
}
.Shopping_order .content{
	margin-bottom:50px;
}
.Shopping .title,.Shopping_order .title,.my_order .title{
    width:100%; 
    height:33px; 
    border-bottom:1px solid #d3d3d3; 
    line-height:33px; 
    background:#fff;
}
.Shopping_order .title .img{
	margin-left: 10px;
}
.Shopping .title .circle{ 
	width:16px; 
	height:16px; 
	margin:0px 10px; 

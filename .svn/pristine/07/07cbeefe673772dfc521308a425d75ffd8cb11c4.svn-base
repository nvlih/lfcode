body{background: #f6f6f6;color: #333; font-family: "Microsoft YaHei" ! important;}


/*-----------------公共样式-----------------------*/

/*浮动*/
.fl {
  float: left;
}
.fr {
  float: right;
}
.cf:before, .cf:after {
  display: table;
  content: " ";
}
.cf:after {
  clear: both;
}
.cf {
  *zoom: 1;
}
.clear{
  clear: both;
}
.hide{
  visibility:hidden;
}

/*头部*/
.pub_header{
  width:100%;
  border-bottom:1px solid #d3d3d3;
  height:45px;
  line-height:45px;
  position:fixed;
  top:0;
  z-index:2;
  font-size:19px;
  background-color: #f5f5f5;
}

.pub_header header ul{
  display:-webkit-box;
}
.pub_header header ul li{
  -webkit-box-flex:1;
  text-align:center;
}
.pub_header header ul li img{
  margin-top:13px;
}
.pub_header .back img{
  width:12px;
  float: left;
  margin-left:10px;
}
.pub_header .info img{
  width:22px;
  float: right;
  margin-right:10px;
  margin-top: 15px;
}

/*导航*/
.pub_nav{
  width:100%;
  height: 96px;
  text-align: center;
  padding-bottom:0;
  color: #838388;
  font-size:12px;  
  background-color: #fff;
}
.pub_nav dl{
  float: left;
  width:25%;
  margin:0 auto;
  height: 100%;
}
.pub_nav dl dt img{
  margin-top: 10px;
}
.pub_nav .nav_bg{
  background: #feeae9;
}
.pub_nav dd{
  margin-top: 10px;
}
.pub_nav dl a{
  color: #32323a;
  display:block;
  padding:0 0 10px 0;
  background-color: #fff;
}
/*按钮样式*/
.btn-hollow{
  display: block;
  color: #323232;
  font-size: 14px;
  line-height: 30px;
  text-align: center;
  width: 80px;
  height: 30px;
  border: 1px solid #95a0aa;
  border-radius: 5px;
}
.btn-hollow-red{
  color: #f86b4f;
  border: 1px solid #f86b4f;
}
.btn{
  display: block;
  width: 90%;
  height: 40px;
  background-color: #f86b4f;
  line-height: 40px;
  text-align: center;
  font-size: 16px;
  color: #fff;
  border-radius: 4px;
  margin: 0 auto;
}
.btn-warn{
  background-image: -moz-linear-gradient(top, #ed3a3a, #d81919); /* Firefox */
  background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0, #ed3a3a), color-stop(1, #d81919)); /* Saf4+, Chrome */
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#ed3a3a', endColorstr='#d81919', GradientType='0'); /* IE*/
}
/*公共按钮——底部*/
.pub_footer{
  width:100%;position:fixed;bottom:0;
  left:0;
  background: #eee;
  border-top:1px solid #dcdcdc;
  text-align: center;
  padding:11px 0;
}
.pub_footer a{
  width:110px;text-align: center;
  color: #fff;font-size:12px;
  background: #f86b4f;
  border:none;padding:8px 0;
  border-radius:20px;
  display:inline-block;
}
.pub_space{
  height:50px;
}
/*弹窗*/
.pic_big{
  background: #000;
  opacity: 0.6;
  filter:alpha(opacity=60);
  width:100%;height:100%;
  position:absolute;left:0;top:0;
  z-index:99;
}
.pic_pop_box{
  height:308px;
  width: 100%;
  background-color: #fff;
  position:absolute;
  left:0;bottom:0;
  z-index:100;
}

/*checkbox优化样式*/
.ui-check {
  position: relative;
  display: inline;
  padding-left: 17px;
  height: 17px;
  font-size: 14px;
  cursor: pointer;
  cursor: hand;
  zoom: 1;
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  -o-user-select: none;
  user-select: none;
}
.check-ico {
  position: absolute;
  left: 0;
  top: 2px;
  width: 17px;
  height: 17px;
  background: url(../images/check.png) 0 0 no-repeat;
  -webkit-background-size: 17px 17px;
  background-size: 17px 17px;
}
.checked-ico {
  background: url(../images/checked.png) 0 0 no-repeat;
  -webkit-background-size: 17px 17px;
  background-size: 17px 17px;
}
.ui-check input {
  position: absolute;
  left: 0;
  top: 2px;
  z-index: 1;
  width: 17px;
  height: 17px;
  border: 0;
  opacity: 0;
  filter: alpha(opacity=0);
  cursor: pointer;
}
.check-label {
  padding-left: 2px;
  color: #5f5f5f;
  cursor: pointer;
}

.Shopping .wrap .left, .Shopping .wrap .center{
    width:10%;
    height:140px; 
    float:left;
}
.Shopping .wrap .left .circle{
    width:16px;
    height:16px; 
    margin:44px 10px;
}
.Shopping .wrap .center{ 
	width: 20%;
}
.Shopping .wrap .center img{ 
	width:100%; 
	margin-top:10px;
}
.Shopping .wrap .right{ 
	width: 68%; 
	float:right; 
	height:140px;
}
.Shopping .wrap .right p.p_1{ 
	width:90%; 
	height:30px; 
	margin-top:10px; 
	line-height:15px; 
	overflow:hidden; 
	font-size:12px; 
}
.Shopping .wrap .right p.p_2{
    width:50%; 
    height:18px; 
    margin-top:18px; 
    font-size:11px; 
    color:#95a0aa;
}
.Shopping .wrap .right p.p_2 span{ 
	font-size:17px; 
	color:#f86b4f; 
	font-weight:bold;
	padding-right:4px;
}
.Shopping .wrap .right .input{ 
	position: relative;
	width:73px; 
	height:23px; 
	border:1px solid #d3d3d3; 
	margin-top:10px; 
	text-align:center; 
	line-height:23px;
}
.Shopping .wrap .right .input a{
	position: absolute;
	display: block;
	right: -88px;
	top: 0;
